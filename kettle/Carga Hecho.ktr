  <step>
    <name>Entrada Tabla Movimientos</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>conexion-bd-fuente</connection>
    <sql>SELECT m.id_producto,
		m.fecha_movimiento,
		m.E_S,
		prod_alm.id_producto,
		prod_alm.id_almacen,
		prod_alm.cantidad_prod_almacen,
		prod_alm.num_ubicaciones,
		sum(m.cantidad) cantidad
		
FROM producto p,
		movimiento m, 
		(SELECT up.id_producto, a.id_almacen, SUM(up.cantidad) cantidad_prod_almacen,
			count(up.id_ubicacion) num_ubicaciones 
			FROM almacen a, ubicacion_producto up, ubicacion u, movimiento m
			WHERE a.id_almacen=u.id_almacen and u.id_ubicacion=up.id_ubicacion
			and up.id_ubicacion=m.id_ubicacion) as prod_alm
WHERE p.id_producto = m.id_producto
		and m.fecha_movimiento between &apos;${fecha_desde}&apos; and &apos;${fecha_hasta}&apos;	

GROUP BY m.id_producto,
		m.fecha_movimiento,
		m.E_S,
		prod_alm.id_producto,
		prod_alm.id_almacen,
		prod_alm.cantidad_prod_almacen,
		prod_alm.num_ubicaciones
ORDER BY m.id_producto asc</sql>
    <limit>0</limit>
